{
    "name": "Cengage.Outline.Speech.htran",
    "displayName": "Speech Outline Tool",
    "version": "0.18",
    "creator": "Cengage Learning",

    "rootURI": "http://localhost:8088/sot/",
    "helpURI": "help.html",

    "features": {
        "supportsTARole": false
    },

    "dockItems": {
        "SOT": {
            "label": "Speech Outlining Tool",
            "icon": "img/dockItem.png",
            "supportsMobile": false,
            "action": {
                "uri": "lti-launch",
                "roles": ["SUPER_ADMIN", "INSTRUCTOR", "PRODUCTION", "VENDOR", "COURSECARE", "TEACHING_ASSISTANT"]
            },
            "parameters": {
            }
        }
    },

    "activities": {
        "SOT": {
            "displayName": "Speech Outlining Tool",
            "description": "Speech Outlining Tool - not editable once student starts",
            "icon": "img/dockItem.png",
            "category": "assignment",
            "placement": "DISTINCT",
            "titleEditable": false,
            "supportsMobile": true,
            "add": {
                "uri": "lti-launch",
                "roles": ["SUPER_ADMIN", "INSTRUCTOR", "PRODUCTION", "VENDOR", "COURSECARE", "TEACHING_ASSISTANT"],
                "parameters": {
                    "lis_result_sourcedid": "$lis_result_sourcedid",
                    "lis_outcome_service_url": "$lis_outcome_service_url"
                }
            },
            "parameters": {
                "hello": "world"
            }
        }
    }
}
